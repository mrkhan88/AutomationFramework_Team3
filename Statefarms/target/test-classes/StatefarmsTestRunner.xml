<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Statefarm TestSuit" parallel="none" thread-count="2">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserStack"/>
    <!--    <parameter name="OS" value="windows"/>-->
    <!--    <parameter name="os_version" value="10"/>-->
    <parameter name="OS" value="windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="89"/>
    <!--        <parameter name="browserName" value="firefox"/>-->
    <!--        <parameter name="browserVersion" value="76.0"/>-->
    <parameter name="url" value="https://www.statefarm.com/"/>

    <test name="Statefarm Automation Test Script using chrome">
        <classes>
            <!--            <class name="statefarm.Testcases.FileClaimTest"/>-->
            <class name="homepageTest.StatehomepageTest"/>
        </classes>
    </test>


</suite>




